<% if current_user %>
  <%= link_to "Log Out" %>
<% else %>
  <%= link_to "Log In", login_path %>
  <%= button_to "Create New User", register_path, method: :get %>
<% end %>

<h3>Existing Users</h3>

<% @users.each do |user| %>
  <div id ="user_<%= user.id %>">
    <%= link_to "#{user.name} - #{user.email}", user_path(user.id) %>
  </div>
<% end %>